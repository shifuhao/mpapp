/* pages/myAddress/index.wxss */
page{
    background-color: #f5f5f5;
}
.list-item{
    margin-top: 25rpx;
    background-color: #ffffff;
}
.info-row{
    padding: 30rpx;
    font-size: 28rpx;
}
.address{
    margin-top: 30rpx;
}
.btn-row{
    padding:0 30rpx;
    height: 88rpx;
    border-top: 1rpx #e1e1e1 solid;
    display: flex;
    align-items: center;
    justify-content: space-between;
    line-height: 88rpx;
    font-size: 26rpx;
    color: #414141;
}
.btn-row-r,
.btn-cell{
    display: flex;
    align-items: center;
}
.btn-cell{
    margin: 20rpx;
}
.btn-icon{
    width: 35rpx;
    height: 35rpx;
    margin: 0 20rpx;
}
.btn-add{
    height:100rpx;
    width: 100%;
    line-height: 100rpx;
    text-align: center;
    background-color: #568a4d;
    color: #ffffff;
    font-size: 32rpx;
    position: fixed;
    bottom: 0;
}