/* pages/finish/index.wxss */

.out {
  min-height: 1200rpx;
  background-color: #f5f5f5;
}

.top {
  width: 750rpx;
  height: 394rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0rpx -1rpx 0rpx 0rpx rgba(238, 238, 238, 1);
  text-align: center;
  border: 1px solid transparent;
}

.icon {
  margin-top: 60rpx;
  margin-bottom: 32rpx;
}

.success {
  font-size: 40rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.tip {
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  margin-top: 16rpx;
}

.bottom {
  text-align: center;
  width: 686rpx;
  line-height: 94rpx;
  background: rgba(234, 104, 25, 1);
  border-radius: 4px;
  margin: 60rpx 32rpx;
  color: #fff;
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
}
