/* components/mine/liebaio/index.wxss */
.liebiao{
  width:750rpx;
height:240rpx;
background:rgba(255,255,255,1);
margin-bottom: 20rpx;
}
.title{
  width: 750rpx;
  border-bottom: 1rpx solid #D8D8D8;
}
.titlein{
  margin: 0 32rpx;
  display: flex;
  justify-content: space-between;
  height: 80rpx;
  align-items: center;
  font-size:28rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(47,47,47,1);
  position: relative;
}
.titlein view:first-child{
  color: #2F2F2F;
}
.titlein view:last-child{
  color: #D8D8D8;
}
.des{
  height: 159rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  text-align: center;
  font-size:24rpx;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(47,47,47,1);
}
.des image{
  width: 48rpx;
  height: 43rpx;
  margin-bottom: 18rpx;
}
.arrow-r{
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  width: 15rpx;
  height: 27rpx;
  position: absolute;
  right: 0;
}