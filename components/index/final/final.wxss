/* 食品清单 */

.finalout {
  margin:0 30rpx;
}

.finalin image {
  width: 336rpx;
  height: 336rpx;
}



.finalin {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;

}

.finaltitle {
  width: 343rpx;
  height: 84rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 42rpx;
}

.finalin .shopcar {
  width: 64rpx;
  height: 64rpx;
}

.finalprice {
  width: 65rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(255, 59, 48, 1);
  line-height: 50rpx;
}

.sellnum {
  height: 33rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 33rpx;
}
.finalbotoom{
  display: flex;
  justify-content: space-between;
